{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "ScriptHost"],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Node",
    "paths": {
      "@leanera/vue-i18n": ["./src/index.ts"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
