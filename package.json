{
  "name": "@byjohann/vue-i18n",
  "type": "module",
  "version": "1.3.1",
  "packageManager": "pnpm@10.11.1",
  "description": "Lightweight internationalization plugin for Vue",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/vue-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/vue-i18n.git"
  },
  "bugs": "https://github.com/johannschopplich/vue-i18n/issues",
  "keywords": [
    "vue",
    "i18n",
    "translation",
    "localization"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.4",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "tsdown": "^0.12.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.0",
    "vue": "^3.5.16",
    "vue-tsc": "^2.2.10"
  }
}
